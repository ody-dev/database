{
  "name": "ody/database",
  "description": "Database package for ody API framework",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilyas Deckers",
      "email": "deckersilyas@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-pdo": "*",
    "illuminate/database": "^11.43",
    "symfony/console": "^5.2.12|^6.0|^7.0",
    "symfony/finder": "^5.0|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.11",
    "ramsey/uuid": "^3.7|^4.0"
  },
  "autoload": {
    "psr-4": {
        "Ody\\DB\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ody\\DB\\Tests\\": "tests/"
    }
  }
