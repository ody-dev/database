{
  "name": "ody/database",
  "description": "Database package for ody API framework",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Ilyas Deckers",
      "email": "deckersilyas@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-pdo": "*",
    "ext-swoole": "^5.1 || ^6.0",
    "illuminate/database": "^11.43",
    "ody/swoole": "dev-master",
    "ody/core": "dev-master",
    "laravel/serializable-closure": "^1.3|^2.0",
    "symfony/console": "^5.2.12|^6.0|^7.0",
    "symfony/finder": "^5.0|^6.0|^7.0",
    "psr/log": "3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.11",
    "ramsey/uuid": "^3.7|^4.0"
  },
  "autoload": {
    "psr-4": {
        "Ody\\DB\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ody\\DB\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../database"
    }
  ]
}
